<root>
	<Interfaces>
		<Interface> 			CombatPropertys			</Interface>
	</Interfaces>

	<Properties>
		<inBattle>
			<Type>			BOOL			</Type>
			<Flags>			ALL_CLIENTS		</Flags>
			<Default>		0				</Default>
		</inBattle>

		<enemyLog>
			<Type>			ENTITYID_LIST		</Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Default>		[]					</Default>
		</enemyLog>
	</Properties>

	<CellMethods>

		<setInBattle>
			<Exposed/>
			<Arg>	BOOL									</Arg>
		</setInBattle>

		<clientRequestUseSkill>
			<Exposed/>
			<Arg>	UUID									</Arg>
			<Arg>	SKILLID									</Arg>
		</clientRequestUseSkill>

		<skillNodeCallServer>
			<Exposed/>
			<Arg>	UUID										</Arg>
			<Arg>	SKILLNODEID									</Arg>
			<Arg>	TABLE										</Arg>
		</skillNodeCallServer>

		<recvDamage>
			<Arg>	ENTITY_ID									</Arg>
			<Arg>	SKILLID										</Arg>
			<Arg>	DAMAGE_TYPE									</Arg>
			<Arg>	HP											</Arg>
		</recvDamage>

		<addEnemy>
			<Arg>	ENTITY_ID									</Arg>
			<Arg>	ENMITY										</Arg>
		</addEnemy>
	</CellMethods>

	<ClientMethods>
		<recvDamage>
			<Arg>	ENTITY_ID									</Arg>
			<Arg>	SKILLID										</Arg>
			<Arg>	DAMAGE_TYPE									</Arg>
			<Arg>	HP											</Arg>
		</recvDamage>

		<skillNodeCallClient>
			<Arg>	UUID										</Arg>
			<Arg>	SKILLNODEID									</Arg>
			<Arg>	TABLE										</Arg>
		</skillNodeCallClient>

	</ClientMethods>
</root>

