	<ClientMethods>
		<recvDamage>
			<Arg>	ENTITY_ID									</Arg>
			<Arg>	SKILLID										</Arg>
			<Arg>	DAMAGE_TYPE									</Arg>
			<Arg>	HP											</Arg>
		</recvDamage>

		<skillNodeCallClient>
			<Arg>	UUID										</Arg>
			<Arg>	SKILLNODEID									</Arg>
			<Arg>	TABLE										</Arg>
		</skillNodeCallClient>

		<serverRequestUseSkill>
			<Arg>	UUID										</Arg>
			<Arg>	SKILLID										</Arg>
		</serverRequestUseSkill>

		<serverSkillFinish>
			<Arg>	SKILLID										</Arg>
		</serverSkillFinish>

		<serverTimeLineFinish>
			<Arg>	UUID										</Arg>
		</serverTimeLineFinish>

	</ClientMethods>
